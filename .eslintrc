{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "installedESLint": true,
  "env": {
    "browser": true
  },
  "plugins": [
      "react",
      "jsx-a11y",
      "import"
  ],
  "rules": {
    "no-tabs": 0,
    "indent": [2, "tab"],
    "import/no-extraneous-dependencies": 0,
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-indent": [2, "tab"],
    "react/jsx-indent-props": [2, "tab"],
  }
}
